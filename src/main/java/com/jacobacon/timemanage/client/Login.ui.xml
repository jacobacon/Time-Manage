<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:res="urn:with:com.jacobacon.timemanage.client.LoginResources">
	<ui:with type="com.jacobacon.timemanage.client.LoginResources" field="res"></ui:with>
	
	<ui:style>

	</ui:style>

	<g:HTMLPanel>
		<div align="center">
			<g:VerticalPanel res:styleName="style.background">
				<g:Label text="Login" res:styleName="style.blackText" />
				<g:TextBox ui:field="loginBox" res:styleName="style.box" />
				<g:Label text="Password" res:styleName="style.blackText" />
				<g:PasswordTextBox ui:field="passwordBox"
					res:styleName="style.box" />
				<g:HorizontalPanel verticalAlignment="middle">
					<g:Button ui:field="buttonSubmit" text="Submit"
						res:styleName="style.loginButton" />
					<g:CheckBox ui:field="myCheckBox" />
					<g:Label ui:field="myLabel" text="Remember me"
						res:styleName="style.blackText" />
				</g:HorizontalPanel>
				<g:Label ui:field="completionLabel1" res:styleName="style.blackText" />
				<g:Label ui:field="completionLabel2" res:styleName="style.blackText" />
			</g:VerticalPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 